<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="iOS Smoke Test Suite" verbose="1" parallel="tests" thread-count="1">
    <parameter name="platform" value="iOS"/>
    <listeners>
        <listener class-name="com.curbside.automation.uifactory.WebDriverListener"/>
    </listeners>
    <test name="iOS- Error-Message">
        <classes>
            <class name="com.capcvs.ios.runners.ErrorMessageTest"/>
        </classes>
    </test>
    <test name="iOS- My-Account">
        <classes>
            <class name="com.capcvs.ios.runners.MyAccountTest"/>
        </classes>
    </test>
    <test name="iOS- MAP">
        <classes>
            <class name="com.capcvs.ios.runners.MapTest"/>
        </classes>
    </test>
    <test name="iOS- Order Details">
        <classes>
            <class name="com.capcvs.ios.runners.OrderDetailsTest"/>
        </classes>
    </test>
    <test name="iOS- TaskManagement">
        <classes>
            <class name="com.capcvs.ios.runners.TaskManagementTest"/>
        </classes>
    </test>
    <test name="iOS- TaskManagement1">
        <classes>
            <class name="com.capcvs.ios.runners.TaskManagement1Test"/>
        </classes>
    </test>
    <test name="iOS- Substitution and Transfer Order">
        <classes>
            <class name="com.capcvs.ios.runners.SubstitutionAndTransferOrderTest"/>
        </classes>
    </test>
    <test name="iOS- Special Symbols">
        <classes>
            <class name="com.capcvs.ios.runners.SpecialSymbolTest"/>
        </classes>
    </test>
    <test name="iOS- Delivery Order">
        <classes>
            <class name="com.capcvs.ios.runners.DeliveryOrderTest"/>
        </classes>
    </test>
    <test name="iOS- Delivery Order">
        <classes>
            <class name="com.capcvs.ios.runners."/>
        </classes>
    </test>
