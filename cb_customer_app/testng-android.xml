<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Test Suite" verbose="1" parallel="methods" thread-count="1">
	<parameter name="platform" value="Android"/>
    <listeners>
        <listener class-name="com.curbside.automation.uifactory.WebDriverListener" />
    </listeners>
    <test name="App Launch">
        <classes>
            <class name="com.curbside.android.runners.AppLaunchTest"/>
        </classes>
    </test>
    <test name="Sign up">
        <classes>
            <class name="com.curbside.android.runners.SignUpTest"/>
        </classes>
    </test>
    <test name="Credit Card">
        <classes>
            <class name="com.curbside.android.runners.CreditCardTest"/>
        </classes>
    </test>
</suite>