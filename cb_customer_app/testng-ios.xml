<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Test Suite" verbose="1" parallel="methods" thread-count="1">
	<parameter name="platform" value="iOS"/>
    <listeners>
        <listener class-name="com.curbside.automation.uifactory.WebDriverListener" />
    </listeners>
    <test name="App Launch">
        <classes>
            <class name="com.curbside.ios.runners.AppLaunchTest"/>
        </classes>
    </test>
    <test name="Sign In">
        <classes>
            <class name="com.curbside.ios.runners.LoginTest"/>
        </classes>
    </test>
    <test name="Sign Up">
        <classes>
            <class name="com.curbside.ios.runners.SignUpTest"/>
        </classes>
    </test>
</suite>