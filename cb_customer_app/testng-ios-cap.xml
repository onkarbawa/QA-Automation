<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="iOS Smoke Test Suite" verbose="1" parallel="tests" thread-count="1">
    <parameter name="platform" value="iOS"/>
    <listeners>
        <listener class-name="com.curbside.automation.uifactory.WebDriverListener" />
    </listeners>
    <test name="iOS- Error-Message">
            <classes>
                <class name="com.cap.ios.runners.ErrorMessage"/>
            </classes>
    </test>
    <test name="iOS- My-Account">
        <classes>
            <class name="com.cap.ios.runners.MyAccount"/>
        </classes>
    </test>
    <test name="iOS- ProductDetails">
        <classes>
            <class name="com.cap.ios.runners.ProductDetailsTest"/>
        </classes>
    </test>
    <test name="iOS- Offline Error-Message">
        <classes>
            <class name="com.cap.ios.runners.OfflineErrorMessageTest"/>
        </classes>
    </test>

</suite>